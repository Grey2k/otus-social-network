{
  "name": "otus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --skip-plugins @vue/cli-plugin-eslint",
    "build": "vue-cli-service build --skip-plugins @vue/cli-plugin-eslint",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "axios": "^0.19.2",
    "bootstrap-vue": "^2.16.0",
    "core-js": "^2.6.5",
    "crypto-js": "^4.0.0",
    "moment": "^2.27.0",
    "normalize.css": "^8.0.1",
    "uuid": "^3.4.0",
    "vue": "^2.6.12",
    "vue-carousel": "^0.18.0",
    "vue-i18n": "^8.21.0",
    "vue-infinite-loading": "^2.4.5",
    "vue-loading-overlay": "^3.4.0",
    "vue-router": "^3.4.3",
    "vue-toast-notification": "^0.4.1",
    "vuex": "^3.5.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^4.5.4",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.12.2",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.20",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/script-indent": [
        "error",
        2,
        {
          "baseIndent": 1
        }
      ],
      "semi": "error"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
